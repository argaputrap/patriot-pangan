<!DOCTYPE html>
<html lang="en">
<head>
    <script type='text/javascript' 
        src='//platform-api.sharethis.com/js/sharethis.js#property=5b3b3edf07712f0011378417&product=sticky-share-buttons' 
        async='async'>
    </script>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="//cdn.datatables.net/1.10.12/css/jquery.dataTables.min.css">
    <title>{{ htmlWebpackPlugin.options.title }}</title>
</head>
<body>
     <header>
        <nav class="navbar navbar-expand-lg">
             <a class="navbar-brand"  href="/">
                <img src="../../../assets/images/brand.png" width="30" height="30" class="d-inline-block align-top"><span class="brand-title ml-2">Patriot Pangan</span>
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon">
                    <i class="fa fa-bars fa-2x navbar-bars"></i>
                </span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item active">
                        <a class="nav-link" href="map.html">Map<span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="statistic.html">Statistic</a>
                    </li>
                    <li class="nav-item nav-item-admin">
                        <a class="nav-link" href="admin.html">Admin</a>
                    </li>
                    <li class="nav-item nav-item-signin">
                        <a class="nav-link nav-signin" data-toggle="modal" data-target="#authModal">
                            Sign In
                        </a>
                    </li>
                    <li class="nav-item nav-item-signout">
                        <a href="#" class="nav-link nav-signout">
                            Sign Out
                        </a>
                    </li>
                </ul>
                </form>
            </div>
        </nav>
    </header>
    <section class="district-table">
        <div class="container district-table">
            <h3 class="district-table__name text-center">
            </h3>
            <hr>
            <p class="district-table__level">

            </p>
            <table id="district-table__dt" style="width:100%" class="display table table-striped district-table__content dt-responsive nowrap">
            </table>
        </div>
    </section>
    <!-- Modal -->
    <div class="modal fade" id="detaiModal" tabindex="-1" role="dialog" aria-labelledby="detaiModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-6 col-sm-12 col-xs-12">
                            <h4>Provinsi</h4>
                        </div>
                        <div class="col-md-6 col-sm-12 col-xs-12">
                            <h4 id="province"></h4>
                        </div>
                        <div class="col-md-6 col-sm-12 col-xs-12">
                            <h4>Kabupaten</h4>
                        </div>
                        <div class="col-md-6 col-sm-12 col-xs-12">
                            <h4 id="regency">: </h4>
                        </div>
                        <div class="col-md-6 col-sm-12 col-xs-12">
                            <h4>Kecamatan</h4>
                        </div>
                        <div class="col-md-6 col-sm-12 col-xs-12">
                            <h4 id="district"></h4>
                        </div>
                    </div>
                    <hr>
                    <p>History Ketahanan Pangan 1 Tahun Terakhir</p>
                    <div class="modal__chart--wrapper">
                        <canvas id="modal__chart" width="600" height="600"></canvas>
                    </div>
                </div>
            </div>
        </div>
    </div>
     <div class="modal fade" id="authModal" tabindex="-1" role="dialog" aria-labelledby="authModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="authModalLabel">Sign In</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="alert alert-danger alert-auth">
                    <b>Email atau password anda salah</b>
                </div>
                <form class="form-auth">
                    <div class="form-group">
                        <label for="email">E-Mail</label>
                        <input type="email" id="email" class="form-control" placeholder="yours@email.com">
                    </div>
                    <div class="form-group">
                        <label for="password">Password</label>
                        <input type="password" id="password" class="form-control" placeholder="yours@email.com">
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary form-auth__button">Login</button>
            </div>
            </div>
        </div>
    </div>
</body>
</html>